{
  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es5",
      "sourceMap": true,
      "strictNullChecks": true,
      "lib": ["esnext", "es2015", "dom", "es2016", "es2017"],
      "removeComments": true,
      "noUnusedLocals": true,
      "declaration": false,
      "outDir": "dist",
      "types": []
  },
  "include": [
      "./cypress",
      "./src"
  ]
}
